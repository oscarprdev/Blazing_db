{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Cloudflare.js",
	"extends": "./base.json",
	"compilerOptions": {
		"plugins": [{ "name": "next" }],
		"module": "es2022",
		"moduleResolution": "Bundler",
		"allowJs": true,
		"jsx": "preserve",
		"noEmit": true,
		"types": ["@cloudflare/workers-types/2023-07-01"]
	}
}
